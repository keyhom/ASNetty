package io.asnetty.util {

/**
 * The log level that <code>InternalLogger</code> can log at.
 *
 * @author Jeremy
 */
public final class InternalLogLevel {

    /** 'TRACE' log level. */
    public static const TRACE:int = 0;
    /** 'DEBUG' log level. */
    public static const DEBUG:int = 1;
    /** 'INFO' log level. */
    public static const INFO:int  = 2;
    /** 'WARN' log level. */
    public static const WARN:int  = 3;
    /** 'ERROR' log level. */
    public static const ERROR:int = 4;

} // class InternalLogLevel
}
